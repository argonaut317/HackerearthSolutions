#include<bits/stdc++.h>
using namespace std;

int main(){
    
    int T;
    cin>>T;
    string str;
    
    for(int i=0;i<T;i++){
        cin>>str;
        int n = str.size();
        int c=0;
        for(int i=0;i<n;i++){
            
            
            switch(str[i]){
                
                case '0':
                    c = c + 6;
                    break;
                case '1':
                    c = c + 2;
                    break;
                case '2':
                    c = c + 5;
                    break;
                case '3':
                    c = c + 5;
                    break;
                case '4':
                    c = c + 4;
                    break;
                case '5':
                    c = c + 5;
                    break;
                case '6':
                    c = c + 6;
                    break;
                case '7':
                    c = c + 3;
                    break;
                case '8':
                    c = c + 7;
                    break;
                case '9':
                    c = c + 6;
                    break;
                    
                
                
                
            }
        }
            
            if(c%2==0){
                
                for(int i=0;i<c/2;i++){
                    cout<<"1";
                }
                
                
            }
            else{
                c = c-3;
                cout<<"7";
                for(int i=0;i<c/2;i++){
                    cout<<"1";
                }
                
            }    
                
            
            
        
        
        cout<<"\n";
        
    }
    
    
    
    
}
