#include<bits/stdc++.h>
using namespace std;

int main(){
    
    int T;
    cin>>T;
    int min;
    cin>>min;
    int arr[T];
    for(int i=0;i<T;i++){
        
        cin>>arr[i];
        
        
    }
    
    for(int i=0;i<T;i++){
        
        if(arr[i]>=min){
            cout<<"YES"<<"\n";
        }
        else{
            cout<<"NO"<<"\n";
        }
    }
    
}
