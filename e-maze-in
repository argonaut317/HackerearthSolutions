#include<bits/stdc++.h>
using namespace std;


int main(){
    
    string str;
    cin>>str;
    
    int n = str.size();
    int x=0;
    int y=0;
    
    for(int i=0;i<n;i++){
        
        if(str[i]=='L'){
            --x;
        }
        else if(str[i]=='R'){
            ++x;
        }
        else if(str[i]=='D'){
            --y;
        }
        else if(str[i]=='U'){
            ++y;
        }
        
    }
    
    cout<<x<<" "<<y;
    
}
